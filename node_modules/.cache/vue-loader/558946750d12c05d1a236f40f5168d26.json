options2: [\n        { id: 1, text: \"S\" },\n        { id: 2, text: \"M\" },\n        { id: 3, text: \"L\" },\n        { id: 4, text: \"XS\" },\n        { id: 5, text: \"XL\" }\n      ]\n    };\n  },\n  methods: {\n    prev() {\n      this.$refs.carousel1.prev();\n    },\n    next() {\n      this.$refs.carousel1.next();\n    },\n    increaseQuantity() {\n      this.quantity++;\n    },\n    decreaseQuantity() {\n      if (this.quantity > 1) {\n        this.quantity--;\n      }\n    }\n  }\n};\n</script>\n<style>\n.product-page .carousel-item {\n  height: 60vh;\n}\n\n.product-page .carousel a.carousel-control-next {\n  right: -20px;\n}\n\n.product-page .carousel a.carousel-control-prev {\n  left: -20px;\n}\n</style>\n"]}]}