button\n            type=\"button\"\n            data-dismiss=\"alert\"\n            aria-label=\"Close\"\n            class=\"close\"\n            @click=\"dismissAlert\"\n          >\n            <span aria-hidden=\"true\">Ã—</span>\n          </button>\n        </slot>\n      </template>\n    </div>\n  </fade-transition>\n</template>\n<script>\nimport { FadeTransition } from \"vue2-transitions\";\n\nexport default {\n  name: \"base-alert\",\n  components: {\n    FadeTransition\n  },\n  props: {\n    type: {\n      type: String,\n      default: \"default\",\n      description: \"Alert type\"\n    },\n    icon: {\n      type: String,\n      default: \"\",\n      description: \"Alert icon\"\n    },\n    dismissible: {\n      type: Boolean,\n      default: false,\n      description: \"Whether alert is closes when clicking\"\n    }\n  },\n  data() {\n    return {\n      visible: true\n    };\n  },\n  methods: {\n    dismissAlert() {\n      this.visible = false;\n    }\n  }\n};\n</script>\n\n<style></style>\n"]}]}