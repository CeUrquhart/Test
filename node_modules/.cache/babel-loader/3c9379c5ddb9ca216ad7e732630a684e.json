AOA;AACA,UAAA,IAAA,EAAA,OADA;AAEA,UAAA,IAAA,EAAA,EAFA;AAGA,UAAA,GAAA,EACA;AAJA,SAPA,CAFA;AAgBA,QAAA,aAAA,EAAA,CAAA,OAAA,EAAA,MAAA,EAAA,MAAA,CAhBA;AAiBA,QAAA,eAAA,EAAA,CAjBA;AAkBA,QAAA,iBAAA,EAAA,GAlBA;AAmBA,QAAA,YAAA,EAAA;AAnBA,OAAA;AAqBA,KAxBA;AAyBA,IAAA,IAzBA,kBAyBA;AACA,WAAA,IAAA,GAAA,KAAA;AACA,WAAA,SAAA,CAAA,IAAA;AACA,KA5BA;AA6BA,IAAA,OA7BA,qBA6BA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,iBAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA,qBAAA;AACA,KAjCA;AAkCA,IAAA,UAlCA,wBAkCA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,iBAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA,qBAAA;AACA,KAtCA;AAuCA,IAAA,QAvCA,oBAuCA,KAvCA,EAuCA;AACA,UAAA,KAAA,GAAA,MAAA,KAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA,qBAAA;AACA,KA3CA;AA4CA,IAAA,KA5CA,mBA4CA;AAAA;;AACA,MAAA,KAAA,CAAA,GAAA,CAAA,gDAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,IAAA,GAAA,GAAA,CAAA,IAAA;AACA,QAAA,KAAA,CAAA,OAAA,GAAA,IAAA;AACA,OAHA;AAIA;AAjDA,GAvBA;AA0EA,EAAA,OA1EA,qBA0EA;AACA,SAAA,KAAA;AACA,GA5EA;AA6EA,EAAA,OA7EA,qBA6EA;AAAA;;AACA,IAAA,MAAA,CAAA,QAAA,GAAA,YAAA;AACA,MAAA,MAAA,CAAA,WAAA,GAAA,MAAA,CAAA,UAAA;AACA,KAFA;AAGA;AAjFA,CAAA","sourcesContent":["<template>\n  <div class=\"\">\n    <modal :show.sync=\"coming\">\n      <p class=\"text-success text-center h4\">Coming Soon!</p>\n    </modal>\n    <modal :show.sync=\"modal\">\n      <div class=\"\">\n        <div class=\"row\">\n          <div class=\"col inspo\">\n            <div\n              class=\"inspo\"\n              :style=\"{ backgroundImage: 'url(' + inspire + ')' }\"\n            ></div>\n          </div>\n        </div>\n        <div style=\"height: 25px;\"></div>\n        <div class=\"row\">\n          <div class=\"col pt-3\" style=\"display: flex;\">\n            <button\n              class=\"btn btn-secondary\"\n              @click=\"spire\"\n              style=\"margin: auto; width: 100%;\"\n            >\n              Another!!\n            </button>\n            <button\n              disabled\n              class=\" btn-transparent\"\n              style=\"border-color: transparent\"\n            ></button>\n            <button\n              class=\"btn btn-secondary\"\n              @click=\"modal = false\"\n              style=\"margin: auto; width: 100%;\"\n            >\n              Close\n            </button>\n          </div>\n        </div>\n        <div class=\"row pt-2\">\n          <div class=\"col\">\n            <p style=\"font-size: 10px; color: gray;\">\n              The inspiration provided is brought to you by Inspirobot, they do\n              not reflect the view or stance of LilBigData.\n            </p>\n          </div>\n        </div>\n      </div>\n    </modal>\n    <transition name=\"slide-fade\">\n      <div\n        class=\"container \"\n        :class=\"{\n          'bg-primary': theme,\n          'bg-dope': !theme,\n          'menu-dark': !isMob,\n          'menu-mob': isMob\n        }\"\n        v-if=\"nav\"\n      >\n        <div class=\"row p-2\">\n          <div\n            class=\"container bg-info\"\n            style=\"border-radius: 15px;\"\n            @click=\"navHome\"\n          >\n            <div class=\"col-12 text-center text-primary p-3\">\n              <i class=\"ni ni-shop\"></i>\n              <p>Home</p>\n            </div>\n          </div>\n        </div>\n\n        <div class=\"row p-2\">\n          <div\n            @click=\"navigate('services')\"\n            class=\"col bg-green container text-center text-primary p-3\"\n            style=\"border-radius: 15px;\"\n          >\n            <i class=\"ni ni-app\"></i>\n            <p>Services</p>\n          </div>\n          <div style=\"width: 10px;\"></div>\n          <div\n            @click=\"navigate('resources')\"\n            class=\"col text-center container bg-warning text-primary p-3\"\n            style=\"border-radius: 15px;\"\n          >\n            <i class=\"ni ni-books\"></i>\n            <p>Resources</p>\n          </div>\n        </div>\n        <div class=\"row p-2\">\n          <div\n            @click=\"navigate('team')\"\n            class=\"col text-center container bg-default text-primary p-3\"\n            style=\"border-radius: 15px;\"\n          >\n            <i class=\"ni ni-circle-08\"></i>\n            <p>Team</p>\n          </div>\n          <div style=\"width: 10px;\"></div>\n          <div\n            @click=\"navContact\"\n            class=\"col text-center container bg-gray text-primary p-3\"\n            style=\"border-radius: 15px;\"\n          >\n            <i class=\"ni ni-bell-55\"></i>\n            <p>Contact</p>\n          </div>\n        </div>\n        <div class=\"row p-2\">\n          <div class=\"container bg-success\" style=\"border-radius: 15px;\">\n            <div\n              class=\"col-12 text-center text-primary p-3\"\n              @click=\"\n                modal = true;\n                spire();\n              \"\n            >\n              <i class=\"ni ni-atom\"></i>\n              <p>Inspire Me</p>\n            </div>\n          </div>\n        </div>\n      </div>\n    </transition>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport Modal from \"@/components/Modal\";\nexport default {\n  components: { Modal },\n  data() {\n    return {\n      primary: this.$store.state.lilbig.nightMode,\n      conf: false,\n      inspire: null,\n      modal: false,\n      coming: false,\n      windowWidth: window.innerWidth\n    };\n  },\n  computed: {\n    nav() {\n      return this.$store.state.lilbig.popNav;\n    },\n    theme() {\n      return this.$store.state.lilbig.nightMode;\n    },\n    isMob() {\n      return this.windowWidth < 500;\n    }\n  },\n  methods: {\n    start() {\n      this.conf = true;\n      this.$confetti.start({\n        canvasId: \"custom-canvas\",\n        particles: [\n          {\n            type: \"image\",\n            size: 10,\n            url:\n              \"https://s3.amazonaws.com/appforest_uf/f1611637190131x354214050123184500/lilbrain50p.png\"\n          },\n          {\n            type: \"image\",\n            size: 15,\n            url:\n              \"https://s3.amazonaws.com/appforest_uf/f1611637190131x354214050123184500/lilbrain50p.png\"\n          }\n        ],\n        defaultColors: [\"white\", \"gray\", \"#fff\"],\n        defaultDropRate: 5,\n        particlesPerFrame: 0.2,\n        windSpeedMax: 0.1\n      });\n    },\n    stop() {\n      this.conf = false;\n      this.$confetti.stop();\n    },\n    navHome() {\n      this.$store.commit(\"deez/warmHeader\");\n      this.$router.push({ path: \"/\" });\n      this.$store.dispatch(\"lilbig/togglePopNav\");\n    },\n    navContact() {\n      this.$store.commit(\"deez/coldHeader\");\n      this.$router.push({ path: \"/\" });\n      this.$store.dispatch(\"lilbig/togglePopNav\");\n    },\n    navigate(value) {\n      const route = \"/\" + value;\n      this.$router.push({ path: route });\n      this.$store.dispatch(\"lilbig/togglePopNav\");\n    },\n    spire() {\n      axios.get(\"https://inspirobot.me/api?generate=true&oy=vey\").then(res => {\n        const data = res.data;\n        this.inspire = data;\n      });\n    }\n  },\n  created() {\n    this.spire();\n  },\n  mounted() {\n    window.onresize = () => {\n      this.windowWidth = window.innerWidth;\n    };\n  }\n};\n</script>\n\n<style scoped>\np,\ni {\n  font-weight: bold;\n  font-size: 18px;\n  color: white;\n}\n.bg-dark {\n  border-color: #5c738a;\n  background-color: #102a43;\n  color: #fff;\n}\n.bg-dope {\n  border-color: #dedede;\n  background-color: #f8f8f8;\n}\n.menu-dark {\n  transform: translate3d(0px, 0px, 0px) scale3d(1, 1, 1) rotateX(0deg)\n    rotateY(0deg) rotateZ(0deg) skew(0deg, 0deg);\n  transform-style: preserve-3d;\n  opacity: 1;\n  display: block;\n  font-family: \"Cerebri Sans\", sans-serif;\n  font-size: 14px;\n  line-height: 20px;\n  -webkit-box-direction: normal;\n  box-sizing: border-box;\n  position: fixed;\n  left: auto;\n  top: auto;\n  right: 2%;\n  bottom: 2%;\n  z-index: 25;\n  width: 357px;\n  height: auto;\n  padding: 30px;\n  border-style: solid;\n  border-width: 1px;\n  border-radius: 20px;\n  background-attachment: scroll !important;\n  box-shadow: none;\n}\n.menu-mob {\n  transform: translate3d(0px, 0px, 0px) scale3d(1, 1, 1) rotateX(0deg)\n    rotateY(0deg) rotateZ(0deg) skew(0deg, 0deg);\n  transform-style: preserve-3d;\n  opacity: 1;\n  display: block;\n  font-family: \"Cerebri Sans\", sans-serif;\n  font-size: 14px;\n  line-height: 20px;\n  -webkit-box-direction: normal;\n  box-sizing: border-box;\n  position: fixed;\n  left: auto;\n  top: 20%;\n  right: auto;\n  bottom: 2%;\n  z-index: 25;\n  width: 330px;\n  height: auto;\n  padding: 20px;\n  border-style: solid;\n  border-width: 1px;\n  border-radius: 20px;\n  background-attachment: scroll !important;\n  box-shadow: none;\n}\n.slide-fade-enter-active {\n  transition: all 0.3s ease;\n}\n.slide-fade-leave-active {\n  transition: all 0.8s ease;\n  transform: translateX(10px);\n}\n.slide-fade-enter, .slide-fade-leave-to\n/* .slide-fade-leave-active below version 2.1.8 */ {\n  transform: translateX(10px);\n  opacity: 0;\n}\n.inspo {\n  height: 350px;\n\n  background-position: center;\n  background-repeat: no-repeat;\n  background-size: cover;\n}\n</style>\n"],"sourceRoot":"src/components"}]}